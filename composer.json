{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "artesaos/seotools": "^1.3",
        "awcodes/filament-curator": "^3.6",
        "awcodes/filament-tiptap-editor": "^3.4",
        "bezhansalleh/filament-language-switch": "^3.1",
        "codezero/laravel-localized-routes": "^4.0",
        "filament/filament": "^3.2",
        "laravel/framework": "^11.32",
        "laravel/tinker": "^2.9",
        "livewire/livewire": "^3.5",
        "lorisleiva/laravel-actions": "^2.8",
        "spatie/laravel-sitemap": "^7.2",
        "spatie/laravel-sluggable": "^3.6",
        "stevebauman/location": "^7.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.14",
        "enlightn/security-checker": "^2.0",
        "fakerphp/faker": "^1.24",
        "larastan/larastan": "^2.9",
        "laravel-lang/common": "^6.4",
        "laravel/dusk": "^8.2",
        "laravel/pint": "^1.17",
        "laravel/sail": "^1.38",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.5",
        "pestphp/pest": "^3.5",
        "pestphp/pest-plugin-faker": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "pestphp/pest-plugin-livewire": "^3.0",
        "phpmd/phpmd": "^2.15",
        "phpro/grumphp": "^2.9",
        "yieldstudio/grumphp-laravel-pint": "^1.0"
    },
    "autoload": {
        "files": [
            "app/Support/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/App/",
            "Domain\\": "app/Domain/",
            "Support\\": "app/Support/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan lang:update"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "codezero/composer-preload-files": true,
            "phpro/grumphp": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
